generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  username  String   @unique
  email     String   @unique
  birthDate DateTime
  gender    String
  phone     String
  name      Name

  @@map("users")
}

type Name {
  first    String
  last     String
  fullName String
}
