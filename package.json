{
  "name": "users-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:MyBillsApp/users-api.git",
  "author": "Davi Seagull <davi.seagull@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/infra/http/server.js NODE_ENV=production",
    "start:dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/infra/http/server.ts' -e ts",
    "prisma:push": "prisma db push",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:push": "yarn test --coverage",
    "prettier:write": "yarn prettier --write \"src/**\" "
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.8.0",
    "serverless-offline": "^13.3.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.414.0",
    "@prisma/client": "^5.8.0",
    "aws-jwt-verify": "^4.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.2.0",
    "shallow-equal-object": "^1.1.1",
    "tsyringe": "^4.8.0"
  }
}
