  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/infra/http/server.js NODE_ENV=production",
    "start:dev": "tsx watch --env-file .env src/infra/http/server.ts",
    "prisma:push": "prisma db push",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:staged": "yarn test --findRelatedTests",
    "test:push": "yarn test --coverage",
    "prettier:write": "yarn prettier --write \"src/**\" "
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.8.0",
    "serverless-offline": "^13.3.2",
    "tsx": "^4.7.0",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.414.0",
    "@prisma/client": "^5.8.0",
    "aws-jwt-verify": "^4.0.0",
    "cls-rtracer": "^2.6.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.2.0",
    "shallow-equal-object": "^1.1.1",
    "tsyringe": "^4.8.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  }
}
